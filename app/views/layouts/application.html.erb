<!DOCTYPE html>
<html>
  <head>
    <title>AdminPanel05</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <header>
        <h1 class="text-center">Welcome Vex Academy</h1>
      </header>
      <div class="row">
        <div class="card">
          <ul class="nav flex-column">
            <li class="nav-item"><%= link_to 'Home', root_path, class: 'nav-link' %></li>
            <li class="nav-item"><%= link_to 'Courses', courses_path, class: 'nav-link' %></li>
            <li class="nav-item"><%= link_to 'Cohorts', cohorts_path, class: 'nav-link' %></li>
            <li class="nav-item"><%= link_to 'Instructors', instructors_path, class: 'nav-link' %></li>
            <li class="nav-item"><%= link_to 'Students', students_path, class: 'nav-link' %></li>
          </ul>
        </div>
        <div class="col-10">
          <div class="">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
      <% flash.each do |key, message| %>
        <% message.each do |m| %>
        <div>
          <%= m %>
        </div>
        <% end %>
      <% end %>
    
  </body>
</html>
